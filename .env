# Environment Variables

# Database (Supabase)
# Replace with your actual Supabase connection string
DATABASE_URL="postgresql://postgres:[PASSWORD]@db.[PROJECT].supabase.co:5432/postgres"

# Auth
# Generate a secure random string for JWT encryption
JWT_SECRET="changeme_this_should_be_a_long_random_string_for_production"

# App URL (used for webhooks)
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Branch configuration
# UUID for the default branch (for single-location setups)
NEXT_PUBLIC_BRANCH_ID="default-branch-id"

# Logging
LOG_LEVEL="info"